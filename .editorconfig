# EditorConfig is awesome: http://EditorConfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true
max_line_length = 80

[*.lua]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.titan]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[titanc]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
