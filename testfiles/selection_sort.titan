function selection_sort(xs: {integer}): ()
    local N: integer = #xs
    for i: integer = 1, N do

        -- Find minimum
        local min_i: integer = i
        local min_x: integer = xs[i]
        for j: integer = i+1, N do
            local y: integer = xs[j]
            if y < min_x then
                min_i = j
                min_x = y
            end
        end

        -- Move it to the front
        local tmp: integer = xs[i]
        xs[i] = xs[min_i]
        xs[min_i] = tmp
    end
end
